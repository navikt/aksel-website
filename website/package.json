    "@navikt/ds-tailwind": "^0.1.4",
    "@navikt/ds-tokens": "^0.8.2",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^3.0.0",
    "@sanity/image-url": "^1.0.1",
    "@sentry/nextjs": "^6.17.6",
    "@svgr/webpack": "^5.5.0",
    "amplitude-js": "^8.8.0",
    "canvg": "^3.0.9",
    "classnames": "^2.3.1",
    "color": "^4.0.2",
    "copy-to-clipboard": "^3.3.1",
    "github-slugger": "^1.4.0",
    "groq": "^2.15.0",
    "js-file-download": "^0.4.12",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^12.1.0",
    "next-sanity": "^0.5.0",
